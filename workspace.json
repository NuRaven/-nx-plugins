{
  "version": 1,
  "projects": {
    "nx-hugo": {
      "root": "packages/nx-hugo",
      "sourceRoot": "packages/nx-hugo/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "packages/nx-hugo/tsconfig.lib.json",
              "packages/nx-hugo/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!packages/nx-hugo/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/nx-hugo/jest.config.js",
            "tsConfig": "packages/nx-hugo/tsconfig.spec.json",
            "passWithNoTests": true
          },
          "outputs": ["coverage/packages/nx-hugo"]
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/nx-hugo",
            "tsConfig": "packages/nx-hugo/tsconfig.lib.json",
            "packageJson": "packages/nx-hugo/package.json",
            "main": "packages/nx-hugo/src/index.ts",
            "assets": [
              "packages/nx-hugo/*.md",
              {
                "input": "./packages/nx-hugo/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/nx-hugo",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/nx-hugo",
                "glob": "builders.json",
                "output": "."
              }
            ]
          },
          "outputs": ["{options.outputPath}"]
        }
      },
      "tags": []
    },
    "nx-hugo-e2e": {
      "projectType": "application",
      "root": "e2e\\nx-hugo-e2e",
      "sourceRoot": "e2e\\nx-hugo-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "nx-hugo:build",
            "npmPackageName": "@nuraven/nx-hugo",
            "pluginOutputPath": "dist/packages/nx-hugo",
            "jestConfig": "e2e/nx-hugo-e2e/jest.config.js",
            "tsSpecConfig": "e2e/nx-hugo-e2e/tsconfig.spec.json"
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["nx-hugo"]
    },
    "nx-strapi": {
      "root": "packages/nx-strapi",
      "sourceRoot": "packages/nx-strapi/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/nx-strapi/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/nx-strapi"],
          "options": {
            "jestConfig": "packages/nx-strapi/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/nx-strapi",
            "tsConfig": "packages/nx-strapi/tsconfig.lib.json",
            "packageJson": "packages/nx-strapi/package.json",
            "main": "packages/nx-strapi/src/index.ts",
            "assets": [
              "packages/nx-strapi/*.md",
              {
                "input": "./packages/nx-strapi/src",
                "glob": "**/!(*.ts)",
                "output": "./src"
              },
              {
                "input": "./packages/nx-strapi/src",
                "glob": "**/*.d.ts",
                "output": "./src"
              },
              {
                "input": "./packages/nx-strapi",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./packages/nx-strapi",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        }
      },
      "tags": []
    },
    "nx-strapi-e2e": {
      "root": "e2e/nx-strapi-e2e",
      "projectType": "application",
      "sourceRoot": "e2e/nx-strapi-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "nx-strapi:build",
            "jestConfig": "e2e/nx-strapi-e2e/jest.config.js"
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["nx-strapi"]
    }
  }
}
